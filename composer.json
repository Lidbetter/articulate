{
  "name": "sprocketbox/articulate",
  "description": "A simple lightweight drop in to replace Eloquent",
  "type": "library",
  "license": "MIT",
  "version": "v1.1.1-alpha",
  "authors": [
    {
      "name": "Ollie Read",
      "email": "ollie@sprocketbox.io"
    }
  ],
  "require": {
    "php": "^7.2",
    "laravel/framework": "~5.6",
    "ramsey/uuid": "^3.7",
    "ext-json": "*"
  },
  "autoload": {
    "psr-4": {
      "Sprocketbox\\Articulate\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "Sprocketbox\\Articulate\\ServiceProvider"
      ]
    }
  },
  "suggest": {
    "sprocketbox/respite": "Allows for the usage of RESTful APIs as a data source"
  }
}
